          },
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "CREATE extension IF NOT EXISTS cube;\nCREATE extension IF NOT EXISTS earthdistance;\n\nDO $$ \nBEGIN\nIF ${start_location:singlequote} = 'none' OR ${destination_location:singlequote} = 'none' THEN\n  RAISE EXCEPTION 'No start or destination location was given, you have to navigate to this dashboard using a link from Trips dashboard';\nEND IF;\nEND $$;\n\nSELECT\n  COUNT(*) AS \"Number of trips\",\n  MAX(\"Distance\") AS \"Distance\",\n  MIN(\"Length\") AS \"Minimum length\",\n  percentile_cont(0.5) WITHIN GROUP (ORDER BY \"Length\") AS \"Median length\",\n  MAX(\"Length\") AS \"Maximum length\",\n  SUM(\"Length\") AS \"Total length\",\n  MIN(\"Duration\") AS \"Minimum duration\",\n  percentile_cont(0.5) WITHIN GROUP (ORDER BY \"Duration\") AS \"Median duration\",\n  stddev_samp(\"Duration\") AS \"Std. dev. duration\",\n  MAX(\"Duration\") AS \"Maximum duration\",\n  SUM(\"Duration\") AS \"Total duration\"\nFROM\n(SELECT\n  id AS \"id\",\n  EXTRACT(EPOCH FROM(\"endDate\"-\"startDate\")) AS \"Duration\",\n  (point(start_locations.longitude, start_locations.latitude)<@>point(destination_location.longitude, destination_location.latitude)) * 1.609344 AS \"Distance\",\n  \"end_mileage_km\"-\"start_mileage_km\" AS \"Length\"\nFROM trips\nLEFT JOIN locations start_locations ON trips.\"start_location_id\" = start_locations.osm_id\nLEFT JOIN locations destination_location ON trips.\"destination_location_id\" = destination_location.osm_id\nWHERE\n  ($__timeFilter(\"startDate\") OR $__timeFilter(\"endDate\")) AND\n  start_location_id = $start_location AND\n  destination_location_id = $destination_location AND\n  vehicle_vin = '$VIN') AS tripdata",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "position_latitude"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "charging_sessions",
          "timeColumn": "connected",
          "timeColumnType": "timestamp",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
      "title": "Details",
      "type": "stat"
    },
