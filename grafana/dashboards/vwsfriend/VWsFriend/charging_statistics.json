        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  soc,\n  MAX(power),\n  AVG(power),\n  MIN(power)\nFROM\n  (\n  SELECT\n    DATE_TRUNC('minute', charges.\"carCapturedTimestamp\") AS \"chargeTime\",\n    charges.\"chargePower_kW\" AS \"power\"\n  FROM charges\n  WHERE\n    charges.\"chargePower_kW\" > 0 AND\n    $__timeFilter(\"carCapturedTimestamp\") AND\n    charges.vehicle_vin = '$VIN'\n  ) AS chargepower\nLEFT JOIN\n  (\n  SELECT\n    DATE_TRUNC('minute', battery.\"carCapturedTimestamp\") AS \"batteryTime\",\n    battery.\"currentSOC_pct\" AS \"soc\"\n  FROM battery\n  WHERE\n    battery.\"currentSOC_pct\" IS NOT NULL AND\n    $__timeFilter(\"carCapturedTimestamp\") AND\n    battery.vehicle_vin = '$VIN'\n  ) AS batterysoc ON \"chargeTime\" = \"batteryTime\"\nGROUP BY soc\nORDER by soc ASC",
          "refId": "A",
          "select": [
